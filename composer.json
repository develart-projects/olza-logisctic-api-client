{
    "name": "develart-projects/olza-logisctic-api-client",
    "description": "Client for Olza Logistic system API",
    "version": "1.7.0",
    "keywords": ["Olza", "OlzaLogistic", "API client", "Olza API client", "DevelArt"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "OlzaApiClient\\": "src/",
            "Test\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "DevelArt",
            "homepage": "https://www.develart.cz/"
        }
    ],  
    "require" : {
      "psr/http-client": "^1.0",
      "psr/http-factory": "^1.0",
      "php": ">=7.1",
      "ext-json": "*"  
    },
    "suggest": {
        "guzzlehttp/guzzle": "Provides http package to the client automaticaly."
    },
    "require-dev": {
        "phpunit/phpunit": "7.5.20"
    },
    "scripts": {
        "test": "vendor/bin/phpunit --verbose --testdox tests"
    }
}
